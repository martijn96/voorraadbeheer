<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Goederenontvangst" table="goederenontvangst">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="datum" type="date" column="datum" nullable="true"/>
    <field name="leverancier" type="string" column="leverancier" length="11" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ordernummer" type="integer" column="ordernummer" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="kwaliteit" type="text" column="kwaliteit" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="artikelnummer" type="text" column="artikelnummer" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="aantal" type="text" column="aantal" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="omschrijving" type="text" column="omschrijving" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
